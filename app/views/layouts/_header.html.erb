<header>
  <nav>
    <div class="header_menu">
      <ul>
        <li><%= link_to "Home", root_path %></li>
        <li class='has-sub'><%= link_to "User Menu", "#" %>
          <ul>
            <% if user_signed_in? %>
              <%= user_menu %>
            <% else %>
              <%= no_user_menu %>
            <% end %>
            <%= omniauth_tag signed_in: user_signed_in? %>
          </ul>
        </li>
        <li class='last'><%= link_to "Menu3", "#" %></li>
      </ul>
    </div>
  </nav>
  <% if notice %>
    <p class="alert alert-notice"><%= notice %></p>
  <% end %>
  <% if alert %>
    <p class="alert alert-error"><%= alert %></p>
  <% end %>
</header>